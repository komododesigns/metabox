
{% set enq = { post_type: 'post-type', posts_per_page: -1, order: 'DESC' } %}
    {% for name in user.field_name %}
    
        <h1>{{ name.title }}</h1>
            <p>Welcome {{ user.display_name }}</p>              
                <table>
                    <thead>
                        <tr>
                            <td>Name</td>
                            <td>location</td>
                            <td>Visit Date</td>
                            <td>Action</td>
                        </tr>
                    </thead>
{% set enquiry = mb.get_posts( enq ) %}
    {% for post in enquiry %}
        {% if post.field_name.title == post1.title %}
                    <tbody>
                        <tr>
                            <td>{{ post.parent_first_name }} {{ post.parent_last_name }}</td>
                            <td>{{ post.field_name.title }}</td>
                            <td>{{ post.visit_date | date( 'd-m-Y' ) }}</td>

                            <td><a href="{{ site.url }}/form-location/?rwmb_frontend_field_post_id={{ post.ID }}" class="table-link">Edit</a> | <a href="{{ post.url }}" class="table-link">View</a></td>
                        
                        </tr>
                    </tbody>
            {% endif %}
        {% endfor %}
    </table> 
    
{% endfor %}
